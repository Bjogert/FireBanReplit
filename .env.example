# FireBanReplit Environment Configuration
# Copy this file to .env.local and adjust settings as needed

# MSB API Configuration
# Set to 'true' to use CORS proxy, 'false' for direct API access
VITE_USE_PROXY=false

# CORS Proxy URL (only used if VITE_USE_PROXY=true)
VITE_PROXY_URL=https://thingproxy-oxuk.onrender.com/fetch/

# MSB API Base URL
VITE_MSB_API_BASE=https://api.msb.se/brandrisk/v2

# Enable debug logging
VITE_DEBUG=false
